{
    "name": "aldidigitalservices/php-mock-collector",
    "description": "A simple Mockserver in PHP that enables you to get the request that was send to the Mockserver (e.g. for Testing)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Hansmann",
            "email": "daniel.hansmann@aldi-sued.com"
        }
    ],
    "require": {
        "php": "~7.4",
        "symfony/http-foundation": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "ALDIDigitalServices\\pms\\": "src/",
            "ALDIDigitalServices\\pms\\rules\\": "src/rules"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
